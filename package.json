{
  "name": "sdp",
  "version": "3.0.3",
  "description": "SDP parsing and serialization utilities",
  "main": "dist/sdp.js",
  "module": "sdp.js",
  "typings": "sdp.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fippo/sdp.git"
  },
  "scripts": {
    "build": "babel sdp.js --presets babel-preset-env --out-dir dist",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov",
    "prepare": "npm run build",
    "test": "eslint sdp.js test/sdp.js && nyc --reporter html mocha test/sdp.js"
  },
  "keywords": [
    "sdp",
    "webrtc"
  ],
  "author": "Philipp Hancke",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.0.0",
    "codecov": "^3.6.5",
    "eslint": "^6.0.1",
    "mocha": "^10.1.0",
    "nyc": "^14.1.1",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.10.0"
  }
}
